<!DOCTYPE html>
<html>
<head>
	<title>Dashboard</title>
	<%= javascript_include_tag 'http://maps.google.com/maps/api/js' %>
	<%= stylesheet_link_tag '//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css' %>
	<%= stylesheet_link_tag 'application', media: 'all' %>
	<%= javascript_include_tag 'application' %>
	<%= csrf_meta_tags %>
	
</head>
<body ng-app="Dashboard">
	<div class="wrapper" ng-view ng-controller="DashboardController">
		<nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0;margin-bottom: 0">
			<div class="navbar-header">
				<a class="navbar-brand" href="index.html"> Philip's Dashboard </a>
			</div>
			<ul class="nav navbar-top-right-links navbar-right">
				<li class="dropdown">
					<a ng-click="addEmployeeLocations()"class="dropdown-toggle" data-toggle="dropdown">
						<i class="fa fa-tasks fa-fw"> </i>
						<i class="fa fa-caret-down"> </i>
					</a>
					<ul class="dropdown-menu dropdown-tasks">
						<li>
							<a href="#">
								<div>
									<i class="fa fa-tasks fa-fw"> </i>
									Employee Locations
									<span class="pull-right text-muted small"> New </span>
								</div>
							</a>
						</li>
						<li class="divider"> </li>
						<li>
							<a href="#">
								<div>
									<i class="icon-dollar"> </i>
									" Sale Flows "
									<span class="pull-right text-muted small"> New </span>
								</div>
							</a>
						</li>
					</ul>
				</li>
			</ul>
		</nav>
		<%= yield %>
	</div>
</body>
</html>
