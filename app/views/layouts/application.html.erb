<!DOCTYPE html>
<html ng-app="Dashboard">
<head>
  <title>Dashboard</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  
  <!-- Using Twitter Bootstrap for custom templates -->
	<link rel="stylesheet" href="bootstrap-sass-official/assets/stylesheets/_bootstrap.scss">
	
  <!-- Using Font Awesome for awesome icons like dollar signs, refresh buttons etc. -->
	<link rel="stylesheet" href="font-awesome/css/font-awesome.min.css">
  
  <!-- Custom CSS for top navigation bar icon placementt, panels etc. -->
	<link rel="stylesheet" href="main.css">
	
  <script src="jquery/dist/jquery.min.js"> </script>
  <script src="angular/angular.min.js"> </script>
  <script src="bootstrap-sass-official/assets/javascript/bootstrap.min.js"> </script>
  <script src="http://maps.google.com/maps/api/js"> </script>
  <script src="ngmap/build/scripts/ng-map.min.js"> </script>
  <script src="angular-chart.js/dist/angular-chart.min.js"> </script>
  
  <script src="module.js"> </script>
</head>
<body>
	<div class="wrapper" ng-view>
		<nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0;margin-bottom: 0">
			<div class="navbar-header">
				<a class="navbar-brand" href="index.html"> Philip's Dashboard </a>
			</div>
			<ul class="nav navbar-top-right-links navbar-right">
				<li class="dropdown">
					<a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
						<i class="fa fa-tasks fa-fw"> </i>
						<i class="fa fa-caret-down"> </i>
					</a>
					<ul class="dropdown-menu dropdown-tasks">
						<li>
							<a href="#">
								<div>
									<i class="fa fa-tasks fa-fw"> </i>
									Employee Locations
									<span class="pull-right text-muted small"> New </span>
								</div>
							</a>
						</li>
						<li class="divider"> </li>
						<li>
							<a href="#">
								<div>
									<i class="icon-dollar"> </i>
									" Sale Flows "
									<span class="pull-right text-muted small"> New </span>
								</div>
							</a>
						</li>
					</ul>
				</li>
			</ul>
		</nav>
		{{1 + 1}}
		<%= yield %>
	</div>
</body>
</html>
